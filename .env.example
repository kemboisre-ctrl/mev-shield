# MEV Shield Environment Configuration
# Copy this file to .env.production and fill in your actual values

# Infura WebSocket URL
INFURA_WSS_URL=wss://mainnet.infura.io/ws/v3/YOUR_INFURA_API_KEY_HERE

# Infura HTTP URL  
INFURA_HTTP_URL=https://mainnet.infura.io/v3/YOUR_INFURA_API_KEY_HERE

# Logging
LOG_LEVEL=info
LOG_FILE=mev_shield.log

# API Configuration
API_PORT=8765
MAX_CONNECTIONS=100
